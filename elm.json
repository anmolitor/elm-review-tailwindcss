{
  "type": "package",
  "name": "anmolitor/elm-review-tailwindcss",
  "summary": "Elm-Review rules for projects using TailwindCSS",
  "license": "BSD-3-Clause",
  "version": "1.0.0",
  "exposed-modules": [
    "TailwindCss.CheckedFunction",
    "TailwindCss.ConsistentClassOrder",
    "TailwindCss.NoCssConflict",
    "TailwindCss.NoUnknownClasses"
  ],
  "elm-version": "0.19.0 <= v < 0.20.0",
  "dependencies": {
    "elm/core": "1.0.5 <= v < 2.0.0",
    "jfmengels/elm-review": "2.13.0 <= v < 3.0.0",
    "stil4m/elm-syntax": "7.2.9 <= v < 8.0.0"
  },
  "test-dependencies": {
    "elm-explorations/test": "2.0.1 <= v < 3.0.0"
  }
}
